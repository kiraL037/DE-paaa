<Window x:Class="DE_Migalkina_22.Edit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DE_Migalkina_22"
        mc:Ignorable="d"
        Title="Edit" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Тип"/>
            <TextBox x:Name="carTypeTextBox"/>
            <Label Content="Модель"/>
            <TextBox x:Name="carModelTextBox"/>
            <Label Content="Описание проблемы"/>
            <TextBox x:Name="problemDescriptionTextBox"/>
            <Button Content="Добавить заявку" Click="SaveButton_Click"/>
        </StackPanel> 
    </Grid>
</Window>
