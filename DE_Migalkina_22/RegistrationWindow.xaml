<Window x:Class="DE_Migalkina_22.RegistrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DE_Migalkina_22"
        mc:Ignorable="d"
        Title="RegistrationWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="ФИО"/>
            <TextBox x:Name="FIOTextBox"/>
            <Label Content="Номер телефона"/>
            <TextBox x:Name="PhoneTextBox"/>
            <Label Content="Логин"/>
            <TextBox x:Name="LoginTextBox"/>
            <Label Content="Пароль"/>
            <PasswordBox x:Name="PasswordTextBox"/>
            <Button Content="Зарегистрироваться" Click="LoginWindowButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
